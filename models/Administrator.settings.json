{
  "connection": "default",
  "collectionName": "strapi_administrator",
  "info": {
    "name": "Administrator",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "required": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "adminLayout": {
      "type": "enumeration",
      "enum": [
        "ADMIN",
        "MANAGER",
        "STAFF"
      ],
      "default": "STAFF",
      "required": true,
      "configurable": false
    }
  }
}
